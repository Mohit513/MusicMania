    <style name="MaterialButtonBaseStyle" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/color_primary</item>
        <item name="cornerRadius">@dimen/_10sdp</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="textAllCaps">false</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:spacing">0dp</item>
        <item name="android:fontFamily" tools:ignore="NewApi">@font/outfit_medium</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
    </style>

    <style name="MaterialButtonBaseStyleWhite" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">@color/color_primary</item>
        <item name="backgroundTint">@color/white</item>
        <item name="cornerRadius">@dimen/_10sdp</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="textAllCaps">false</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:spacing">0dp</item>
        <item name="android:fontFamily" tools:ignore="NewApi">@font/outfit_medium</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_40sdp</item>
        <item name="strokeColor">@color/color_primary</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="CustomBottomSheetStyle" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/CustomBottomSheet</item>
       <item name="background">@android:color/transparent</item>
    </style>

    <style name="CustomBottomSheet" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:layout_margin">@dimen/_16sdp</item>
        <item name="background">@android:color/transparent</item>
        <item name="cornerRadius">@dimen/_12sdp</item>
    </style>

